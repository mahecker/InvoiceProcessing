<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
<!--       <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/> -->
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="de.fhm.DataProcessing.InvoiceDataProcessing" level="trace" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="de.fhm.DataSource.InvoiceDataSource" level="trace" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="de.fhm.DataFormat.InvoiceDataFormat" level="trace" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="de.fhm.DataOutput.InvoiceDataOutput" level="trace" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Root level="error">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>